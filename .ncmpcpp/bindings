def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "ctrl-d"
   page_down 
def_key "ctrl-u"
   page_up 

# not used but bound
def_key "J"
    move_selected_items_down
def_key "K"
    move_selected_items_up
