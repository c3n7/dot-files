{
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "args": ["--background-index"],
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "lua": {
      "command": "java",
      "args": [
        "-cp",
        "/home/timo/.programs/vim-utils/EmmyLua-LS-all.jar",
        "com.tang.vscode.MainKt"
      ],
      "filetypes": ["lua"],
      "rootPatterns": [".git/"]
    }
  },
  "[javascript][[typescript][typescriptreact][javascriptreact][json][javascriptreact][graphql][html][htmldjango][blade][dart][go][java][php][yaml][lua][css][xml][python][scss][vue]": {
    "coc.preferences.formatOnSave": true
  },
  "prettier.bracketSpacing": true,
  "prettier.singleQuote": false,
  "prettier.printWidth": 80,
  "prettier.proseWrap": "always",
  "html.format.wrapLineLength": 60,
  "jedi.startupMessage": true,
  "jedi.hover.enable": true,
  "blade.bladeFormatter.optIndentSize": 4,
  "intelephense.diagnostics.undefinedMethods": false,
